  
　<h5>会員一覧</h5>
　
　<div class = "col-md-8">

  <table class = "table table-hover table-inverse">
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
    
    <% @customers.each do |customer| %>

    <tr>
      <td><%= customer.id %></td>
      <td><%= link_to customer.name, admin_customer_path(customer.id) %></td>
      <td><%= customer.email %></td>
      <td>
        <% if customer.is_delete == true %>
          有効
        <% else %>
          退会
        <% end %>
      </td>
    </tr>

  <% end %>

  </table>
  
  <%= paginate @customers %>
  
  </div>

　